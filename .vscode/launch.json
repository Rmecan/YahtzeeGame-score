{
  "version": "0.2.0",
  "type": "setting",
  "settings": {
    "justMyCode": false
  },
  "configurations": [
      {
          "name": ".NET Core Launch (console)",
          "type": "coreclr",
          "request": "launch",
          "preLaunchTask": "dotnet: clean; dotnet: build",
          "program": "${workspaceFolder}/bin/Debug/net9.0/YahtzeeGame.dll",
          "args": [],
          "cwd": "${workspaceFolder}",
          "stopAtEntry": false,
          "console": "internalConsole",
          "internalConsoleOptions": "openOnSessionStart",
          "launchBrowser": {
              "enabled": false
          },
          "env": {
              "ASPNETCORE_ENVIRONMENT": "Development"
          },
          "sourceFileMap": {
              "/Views": "${workspaceFolder}/Views"
          }
      }
  ]
}